from "join.in" import join

fun foo()
    print("Fib!")
end

fun add(a,b)
    return a+b
end

fun fib(n)
    if n < 2
        return n
    else
        return fib(n-1)+fib(n-2)
    end
end

fun main(args)
    const t1 := spawn(fib,5)
    const t2 := spawn(fib,5)
    assert(8 == fib(6))
    join(t1)
    join(t2)
    foo()
    const v := add(2,3)
    print(args[0]," ",args[1])
    assert(v == 5)
end
